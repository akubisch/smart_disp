pars <- as.numeric(apply(as.matrix(read.table("pop_3.in")),2,mean))
dde_ta <- function(para) {
c <- seq(0,2,len=100)
d <- 1.0 - para[1]/c
d[c<para[1]] <- 0
return(d)
}
ta <- dde_ta(pars)
plot(ta)
pars <- as.numeric(apply(as.matrix(read.table("pop_4.in")),2,mean))
dde_tae <- function(para) {
c <- seq(0,2,len=100)
d <- 1.0 - para[1] * (1.0 - para[2])/(c - para[1] * para[2])
d[c<para[1]] <- 0
return(d)
}
tae <- dde_tae(pars)
pars
