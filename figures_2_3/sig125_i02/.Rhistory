LENGTHI <- 100
# 1. TL
pars <- as.numeric(apply(as.matrix(read.table("populations/pop_2.in")),2,mean))
dde_tl <- function(para) {
c <- seq(0,2,len=LENGTHI)
d <- para[2]*c+para[1]
d[d<0] <- 0
d[d>1] <- 1
return(d)
}
tl <- dde_tl(pars)
# 2. TA
pars <- as.numeric(apply(as.matrix(read.table("populations/pop_3.in")),2,mean))
dde_ta <- function(para) {
c <- seq(0,2,len=LENGTHI)
d <- 1.0 - para[1]/c
d[c<para[1]] <- 0
return(d)
}
ta <- dde_ta(pars)
# 3. TAe
pars <- as.numeric(apply(as.matrix(read.table("populations/pop_4.in")),2,mean))
dde_tae <- function(para) {
c <- seq(0,2,len=LENGTHI)
d <- 1.0 - para[1] * (1.0 - para[2])/(c - para[1] * para[2])
d[c<para[1]] <- 0
d[d<0] <- 0
d[d>1] <- 1
return(d)
}
tae <- dde_tae(pars)
# 4. S
pars <- as.numeric(apply(as.matrix(read.table("populations/pop_5.in")),2,mean))
dde_s <- function(para) {
c <- seq(0,2,len=LENGTHI)
d <- para[3]/(1.0+exp(-para[1]*(c-para[2])))
return(d)
}
s <- dde_s(pars)
# plot
postscript(file="strategies.eps",width=7,height=5,paper="special",horizontal=F,pointsize=12)
par(mar=c(5,5,.5,.5))
plot(1,1,type="n",bty="l",xlim=c(0,2),ylim=c(0,1),xlab="N/K",ylab="emigration propensity",cex.axis=1.5,cex.lab=1.5)
c <- seq(0,2,len=LENGTHI)
lines(tl~c,lwd=2,col="green")
lines(ta~c,lwd=2,col="black")
lines(tae~c,lwd=2,col="blue")
lines(s~c,lwd=2,col="red")
dev.off()
