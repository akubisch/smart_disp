setwd("~/Desktop/woes76/sig0_i02")
LENGTHI <- 100
# 1. TL
pars <- as.numeric(apply(as.matrix(read.table("populations/pop_2.in")),2,mean))
dde_tl <- function(para) {
c <- seq(0,2,len=LENGTHI)
d <- para[2]*c+para[1]
d[d<0] <- 0
d[d>1] <- 1
return(d)
}
tl <- dde_tl(pars)
# 2. TA
pars <- as.numeric(apply(as.matrix(read.table("populations/pop_3.in")),2,mean))
dde_ta <- function(para) {
c <- seq(0,2,len=LENGTHI)
d <- 1.0 - para[1]/c
d[c<para[1]] <- 0
return(d)
}
ta <- dde_ta(pars)
